                <th>ID</th>
                <th>Fecha</th>
                <th>Hora</th>
                <th>Descripci√≥n</th>
            </tr>
        </thead>
        <tbody>
            <% citas.forEach(cita => { %>
                <tr>
                    <td><%= cita.id %></td>
